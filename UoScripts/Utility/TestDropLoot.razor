    hotkey 'Set Grab Item HotBag'
    wft 250
    target deposit
endif
if findtype 'crate' ground 0 0 2 as lootcrate
    dclick lootcrate
    pause 250
    while findtype 'muffins' lootcrate as mufs
        hotkey 'Grab Item'
        wft 250
        target mufs
    endif
endif
pause 500
